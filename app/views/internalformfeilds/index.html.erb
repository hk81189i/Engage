<h1>Listing internalformfeilds</h1>

<table>
  <thead>
    <tr>
      <th>Internalform</th>
      <th>Customftypes</th>
      <th>Customf</th>
      <th>Required</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @internalformfeilds.each do |internalformfeild| %>
      <tr>
        <td><%= internalformfeild.internalform_id %></td>
        <td><%= internalformfeild.customftype_id %></td>
        <td><%= internalformfeild.cfeild%></td>
