        <td><%= link_to 'Show', internalformfeild %></td>
        <td><%= link_to 'Edit', edit_internalformfeild_path(internalformfeild) %></td>
        <td><%= link_to 'Destroy', internalformfeild, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

