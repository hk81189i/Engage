<h1>Listing pconfirms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @pconfirms.each do |pconfirm| %>
      <tr>
        <td><%= pconfirm.name %></td>
        <td><%= pconfirm.email %></td>
        <td><%= pconfirm.phone %></td>
        <td><%= link_to 'Show', pconfirm %></td>
        <td><%= link_to 'Edit', edit_pconfirm_path(pconfirm) %></td>
        <td><%= link_to 'Destroy', pconfirm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pconfirm', new_pconfirm_path %>
